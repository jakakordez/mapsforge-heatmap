<?xml version="1.0" encoding="UTF-8"?>
<rendertheme xmlns="http://mapsforge.org/renderTheme" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://mapsforge.org/renderTheme renderTheme.xsd" version="1" map-background="#2E3141">

    <rule e="way" k="natural" v="issea|sea">
        <area fill="#add8e6" />
    </rule>

    <rule e="way" k="natural" v="nosea">
        <area fill="#eeeeee" />
    </rule>

    <rule e="way" k="waterway" v="river">
        <line stroke="#add8e6" stroke-width="1" />
    </rule>

    <rule e="way" k="boundary" v="administrative">
        <rule e="way" k="admin_level" v="2">
            <line stroke="#555555" stroke-width="1" />
        </rule>
    </rule>

    <rule e="way" k="natural" v="water" closed="yes">
        <area fill="#add8e6" />
    </rule>

    <rule e="node" k="place" v="village" zoom-min="13">
        <caption k="name" font-size="10" stroke="#aaaaaa" fill="#aaaaaa" stroke-width="0.0" />
    </rule>

    <rule e="node" k="place" v="town" zoom-min="10">
        <caption k="name" font-size="12" stroke="#cccccc" fill="#cccccc" stroke-width="0.0" />
    </rule>

    <rule e="node" k="place" v="suburb" zoom-min="13">
        <caption k="name" font-size="12" stroke="#cccccc" fill="#cccccc" stroke-width="0.0" />
    </rule>

    <rule e="node" k="place" v="city" zoom-min="7" zoom-max="14">
        <caption k="name" font-size="14" stroke="#ffffff" fill="#ffffff" stroke-width="0.0" />
    </rule>

    <rule e="node" k="place" v="country" zoom-max="6">
        <caption k="name" font-style="bold" font-size="15" stroke="#ffffff" fill="#000000" stroke-width="1.0" />
    </rule>
</rendertheme>